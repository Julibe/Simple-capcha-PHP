Options -MultiViews
php_value upload_max_filesize 10M
php_value post_max_size 10M
RewriteEngine On
RewriteCond %{REQUEST_URI} \.(jpg|gif|png)$ [NC]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d

#RewriteBase /

RewriteRule ^$ index.php
RewriteRule ^(.*).admin$ index.php?admin=true&section=$1&type=html [QSA,L]
RewriteRule ^(.*).html$ index.php?section=$1&type=html [QSA,L]
RewriteRule ^(.*).int.html$ index.php?section=$1&type=int [QSA,L]
RewriteRule ^(.*).jss$ index.php?section=$1&type=js [QSA,L]
RewriteRule ^capcha.png$ capcha.php [QSA,L]